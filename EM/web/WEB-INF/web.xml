<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>RegistServlet</servlet-name>
        <servlet-class>cn.tedu.web.RegistServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValiImgServlet</servlet-name>
        <servlet-class>cn.tedu.web.ValiImgServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjaxHasUsernameServlet</servlet-name>
        <servlet-class>cn.tedu.web.AjaxHasUsernameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistServlet</servlet-name>
        <url-pattern>/RegistServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValiImgServlet</servlet-name>
        <url-pattern>/ValiImgServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjaxHasUsernameServlet</servlet-name>
        <url-pattern>/AjaxHasUsernameServlet</url-pattern>
    </servlet-mapping>
</web-app>